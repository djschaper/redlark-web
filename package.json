{
  "name": "FBC-Redlark",
  "version": "0.1.3",
  "private": true,
  "scripts": {
    "build": "node -e \"require('./bin/deploy.js').build()\"",
    "build:deploy": "node -e \"require('./bin/deploy.js').buildAndDeploy()\"",
    "deploy": "node -e \"require('./bin/deploy.js').deploy()\"",
    "start": "cd src && node app.js",
    "start:dev": "cd src && nodemon --require dotenv/config app.js -e js,mjs,json,html dotenv_config_path=../.env",
    "sync:db": "cd bin && node ./dbSync.js"
  },
  "dependencies": {
    "mysql2": "^1.5.3",
    "password-hash": "^1.2.2",
    "sequelize": "^4.38.0"
  },
  "devDependencies": {
    "archiver-promise": "^1.0.0",
    "aws-sdk": "^2.270.1",
    "bluebird": "^3.5.1",
    "dotenv": "^6.0.0",
    "fs-extra": "^6.0.1",
    "nodemon": "^1.17.5",
    "prompt-promise": "^1.0.3"
  }
}
