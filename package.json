{
  "name": "fbc-redlark",
  "version": "0.3.1",
  "private": true,
  "scripts": {
    "build": "node -e \"require('./bin/deploy.js').build()\"",
    "build:deploy": "node -e \"require('./bin/deploy.js').buildAndDeploy()\"",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:seed": "sequelize db:seed:all",
    "db:seed:undo": "sequelize db:seed:undo --seed",
    "deploy": "node -e \"require('./bin/deploy.js').deploy()\"",
    "generate:password": "node -e \"console.log(require('password-hash').generate(process.argv[1]))\"",
    "meh": "cd meh && nodemon meh.js",
    "migration:create": "sequelize migration:create --name",
    "seed:create": "sequelize seed:create --name",
    "start": "sequelize db:migrate && sequelize db:seed:all && cd src && node app.js",
    "start:dev": "cd src && nodemon app.js -e js,mjs,json,html"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "dotenv": "^6.0.0",
    "glob": "^7.1.2",
    "googleapis": "^32.0.0",
    "mysql2": "^1.5.3",
    "password-hash": "^1.2.2",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.0.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "archiver-promise": "^1.0.0",
    "aws-sdk": "^2.270.1",
    "bluebird": "^3.5.1",
    "dropbox": "^4.0.7",
    "fs-extra": "^6.0.1",
    "isomorphic-fetch": "^2.2.1",
    "nodemon": "^1.17.5",
    "prompt-promise": "^1.0.3"
  },
  "beanstalkPackage": [
    "src/",
    "db/",
    "package.json",
    ".ebextensions",
    ".sequelizerc"
  ]
}
